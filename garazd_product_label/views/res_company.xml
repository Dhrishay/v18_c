<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="res_garazd_product_label_view_company" model="ir.ui.view">
        <field name="name">res.company.garazd_product_label</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='phone']" position="after">
                <field name="label_bg_color"  widget="color"></field>
                <field name="product_label_template_id"></field>
                <field name="product_label_template_name"></field>
            </xpath>
        </field>
    </record>

</odoo>
