<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_agreement_billboard" model="ir.sequence">
        <field name="name">Reference</field>
        <field name="code">agreement.billboard</field>
        <field name="prefix">KKM/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="product_category_rental" model="product.category">
        <field name="name">Rental Service</field>
    </record>
    <record id="rental_product" model="product.product">
        <field name="name">Rental</field>
        <field name="categ_id" ref="product_category_rental"/>
        <field name="type">service</field>
        <field name="default_code">RENTAL</field>

    </record>
</odoo>