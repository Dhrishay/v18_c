<!-- Owner Tag -->
    <!-- Owner Tag Tree View -->
    <record id="view_owner_tag_tree" model="ir.ui.view">
        <field name="name">owner.tag.tree</field>
        <field name="model">owner.tag</field>
        <field name="arch" type="xml">
            <list>
                <field name="owner_tag_id"/>
            </list>
        </field>
    </record>

    <!-- Owner Tag Form View -->
    <record id="view_owner_tag_form" model="ir.ui.view">
        <field name="name">owner.tag.form</field>
        <field name="model">owner.tag</field>
        <field name="arch" type="xml">
            <form>
                <sheet> 
                    <group>
                        <group>
                            <field name="owner_tag_id"/>
                            <field name="name"/>
                            <field name="owner_sale_order"/>
                            <field name="owner_contract"/>
                        </group>
                    </group>

                    <notebook>
                        <page string="Owner Sale Order" name="owner_sale_order">
                            <group>
                                <field name="owner_image" widget="image" style="max-width:150px;"/>
                                <field name="owner_text_header"/>
                            </group>
                            <group>
                                <field name="owner_text_footer"/>
                            </group>
                        </page>
                        <page string="Owner Contract" name="owner_contract">
                            <group>
                                <field name="contract_image_id" widget="image" style="max-width:150px;"/>
                                <field name="contract_header_id"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Owner Tag Action -->
    <record id="action_menu_owner_tag" model="ir.actions.act_window">
        <field name="name">Owner Tags</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">owner.tag</field>
        <field name="view_mode">list,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first owner!
            </p>
        </field>
    </record>

