<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_config_view_form" model="ir.ui.view">
        <field name="name">res_company_config_view_form_inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <label for="delivery_time" steing="Delivery Time"/>
                <div>
                    <field name="delivery_time" style="max-width: 5rem;text-align: center;"/> Minutes
                </div>
            </xpath>
        </field>
    </record>
</odoo>
