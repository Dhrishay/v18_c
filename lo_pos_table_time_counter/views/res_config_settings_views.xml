<?xml version="1.0"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.pos_restaurant</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='restaurant_section']" position="inside">
                <setting help="Table Time Counter">
                    <field name="pos_table_timer_enable" string="Table Time Counter"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
